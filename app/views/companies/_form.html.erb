<%= form_for(company) do |f|%>
    <div class="d-flex">
        <div class="m-3">
            <%= f.label :"기수"%>
            <%= f.number_field :season %>
        </div>
        <div class="m-3">
            <%= f.label :"상호"%>
            <%= f.text_field :name %>
        </div>
        <div class="m-3">
            <%= f.label :"상호(영문)"%>
            <%= f.text_field :name_en %>
        </div>
    </div>
    <div class="d-flex">
        <div class="m-3">
            <%= f.label :"서비스명"%>
            <%= f.text_field :service_name %>
        </div>
        <div class="m-3">
            <%= f.label :"서비스명(영문)"%>
            <%= f.text_field :service_name_en %>
        </div>
        <div class="m-3">
            <%= f.label :"기업 로고 (jpg, jpeg, png 파일)"%>
            <%= f.file_field :logo, accept: '.jpg , .jpeg , .png' %>
        </div>
        <div class="m-3">
            <%= f.label :"기업 로고(Ai 파일)"%>
            <%= f.file_field :logo_ai, accept: '.ai' %>
        </div>
        
    </div>
    <div class="d-flex">
        <div class="m-3">
            <%= f.label :"사업자등록여부"%>
            <label><%= f.radio_button :isRegistered, true %>Y</label>
            <label><%= f.radio_button :isRegistered, false %>N</label>
        </div>
        <div class="m-3">
            <%= f.label :"사업자 등록 번호"%>
            <%= f.number_field :register_num, disabled: "disabled"  %>
        </div>
        <div class="m-3">
            <%= f.label :"기업 설립 날짜"%>
            <%= f.date_field :established_date %>
        </div>
    </div>
    <div class="d-flex">
        <div class="m-3">
            <%= f.label :"법인 유무"%>
            <label><%= f.radio_button :isCorporated, true %>Y</label>
            <label><%= f.radio_button :isCorporated, false %>N</label>
        </div>
        <div class="m-3">
            <%= f.label :"법인 등록 번호"%>
            <%= f.number_field :corporated_num %>
        </div>
        <div class="m-3">
            <%= f.label :"법인 설립 날짜"%>
            <%= f.date_field :corporated_date %>
        </div>
    </div>
    <div class="d-flex">
        <div class="m-3">
            <%= f.label :"공동 대표 여부"%>
            <label><%= f.radio_button :co_CEO, true%>Y</label>
            <label><%= f.radio_button :co_CEO, false %>N</label>
        </div>
        <div class="m-3">
            <%= f.label :"대표자명"%>
            <%= f.text_field :CEO %>
        </div>
        <div class="m-3">
            <%= f.label :"주소"%>
            <%= f.text_field :address %>
        </div>
    </div>
    <div class="d-flex">
        <div class="m-3">
            <%= f.label :"기업 개요"%>
            <%= f.text_area :company_overview %>
        </div>
        <div class="m-3">
            <%= f.label :"서비스 개요"%>
            <%= f.text_area :service_overview %>
        </div>
    </div>
    <div class="d-flex">
        <div class="m-3">
            <%= f.label :"웹사이트"%>
            <%= f.text_field :website %>
        </div>
        <div class="m-3">
            <%= f.label :"SNS"%>
            <%= f.text_field :sns %>
        </div>
        <div class="m-3">
            <%= f.label :"휴대폰 번호"%>
            <%= f.number_field :cellphone_num %>
        </div>
        <div class="m-3">
            <%= f.label :"일반전화 번호"%>
            <%= f.number_field :phone_num %>
        </div>
        <div class="m-3">
            <%= f.label :"팩스 번호"%>
            <%= f.number_field :fax_num %>
        </div>
    </div>
    <div class="d-flex">
        <div class="m-3">
            <%= f.label :"입주 날짜"%>
            <%= f.date_field :move_in_date %>
        </div>
        <div class="m-3">
            <%= f.label :"퇴주 날짜"%>
            <%= f.date_field :move_out_date %>
        </div>
        <div class="m-3">
            <%= f.label :"경영대생 유무"%>
            <%= f.text_field :isBusinessStudent %>
        </div>
        <div class="m-3">
            <%= f.label :"멘토링 횟수"%>
            <%= f.number_field :mentoring %>
        </div>
    </div>
    <div class="d-flex">
        <div class="m-3">
            <%= f.label :"EXPRESS 자료(pdf 파일)"%>
            <%= f.file_field :express_file, accept: ".pdf" %>
        </div>
        <div class="m-3">
            <%= f.label :"발표자료(pdf, ppt, pptx 파일)"%>
            <%= f.file_field :presentation, accept: ".pdf , .ppt , .pptx" %>
        </div>
    </div>
    
        <div class="m-3">
            <%= f.submit %>
        </div>
    </div>
<% end %>

<script>
    $("#")
</script>