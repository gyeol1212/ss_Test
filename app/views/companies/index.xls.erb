<table class="table">
    <thead>
      <tr>
        <th>No.</th>
        <th>기수</th>
        <th>기업명</th>
        <th>서비스명</th>
        <th>연락처</th>
        <th>사업자 등록 여부</th>
        <th>법인 등록 여부</th>
        <th>경영대생 유무</th>

      </tr>
    </thead>
    <tbody>
      <% @companies.each do |company| %>
        <tr>
          <td><%= company.id %></td>
          <td><%= company.season %></td>
          <td><%= company.name %></td>
          <td><%= company.service_name %></td>
          <td><%= company.cellphone_num %></td>
          <td><%= company.isRegistered %></td>
          <td><%= company.isCorporated %></td>
          <td><%= company.isBusinessStudent %></td>
          
        </tr>
      <% end %>
    </tbody>
</table>
